@model IEnumerable<Bsnsapp.Models.Vendor>

@{
    ViewBag.Title = "vendor";
    Layout = "~/Views/Shared/_VendorLayout.cshtml";
}

<h2>vendor</h2>
 @Html.ActionLink("Add Business", "CreateVendor")
<table>
    <tr>
        <th>
            BusinessTitles
        </th>
        <th>
            CompanyName
        </th>
        <th>    
            Email
        </th>
        <th>
            Name
        </th>
        <th>
            Address1
        </th>
        <th>
            Address2
        </th>
        <th>
            City
        </th>
        <th>
            State
        </th>
        <th>
            Pin
        </th>
        <th>
            Longitude
        </th>
        <th>
            Latitude
        </th>
        <th>
            Country
        </th>
        <th>
            Phone
        </th>
        <th></th>
    </tr>

@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BusinessTitles)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CompanyName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Longitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Latitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Country)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.ActionLink("Edit", "VendorEdit", new { id=item.Id }) |
            @Html.ActionLink("Details", "VendorDetails", new { id=item.Id }) |
            @Html.ActionLink("Delete", "VendorDelete", new { id=item.Id })
        </td>
    </tr>
}

</table>


@model IEnumerable<Bsnsapp.Models.Mov>
 @{
     ViewBag.Title = "Home";
     Layout = "~/Views/Shared/_VendorLayout.cshtml";
}


<!DOCTYPE html>

<html>
<head>
    <title>Index</title>
</head>
<body>
    @*<p>
        @Html.ActionLink("Create New", "CreateUser")
        <a>&nbsp;&nbsp;|&nbsp;&nbsp;</a>
       
    </p>*@
    
    <table cellpadding="0" cellspacing="10px" style="width:100%; font-size:12px;" >
        <tr style=" border-bottom:1px solid black; border-top:1px solid black;">
            <th></th>
<th></th>
<th></th>
            <th>
                Email
            </th>
            <th>
                Name
            </th>
            <th>
                Address Line 1
            </th>
            <th>
                Address Line 2
            </th>
            <th>
                City
            </th>
            <th>
                State
            </th>
            <th>
                Pin
            </th>
            <th>
                Country
            </th>
            <th>
                Phone
            </th>
            <th>
                Dob
            </th>
           
        </tr>
    
    @foreach (var item in Model)
    {
        <tr style=" border-bottom: 1px solid black ">
              <td style=" color: Green; font-weight:bold;">
                @Html.ActionLink("Edit", "EditUser", new { id = item.Id })</td> 
                 <td style=" color: Green;font-weight:bold;">
                @Html.ActionLink("Details", "DetailsUser", new { id = item.Id }) </td>
                 <td style=" color: Green;font-weight:bold;">
                @Html.ActionLink("Change Password","ChangePassword", new { id = item.Id }) 
            </td>
            <td>
                @item.Email
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Address1
            </td>
            <td>
                @item.Address2
            </td>
            <td>
                @item.City
            </td>
            <td>
                @item.State
            </td>
            <td>
                @item.Pin
            </td>
            <td>
                @item.Country
            </td>
            <td>
                @item.Phone
            </td>
            <td>
                @item.Dob
            </td>
           
        </tr>
    }
    
    </table>
